/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=b, b=sel, out=bandc);
    Not(in=sel, out=notc);
    And(a=a, b=notc, out=aandnotc);
    Or(a=aandnotc, b=bandc, out=out);
}